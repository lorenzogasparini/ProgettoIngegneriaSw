/></table><table schema="main" name="rilevazione_sintomo" show_row_id="0" encoding="" plot_x_axis="" unlock_view_pk="_rowid_" freeze_columns="0"><sort/><column_widths><column index="1" value="44"/><column index="2" value="90"/><column index="3" value="234"/><column index="4" value="148"/><column index="5" value="68"/></column_widths><filter_values/><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table><table schema="main" name="terapia" show_row_id="0" encoding="" plot_x_axis="" unlock_view_pk="_rowid_" freeze_columns="0"><sort/><column_widths><column index="1" value="44"/><column index="2" value="88"/><column index="3" value="122"/><column index="4" value="140"/><column index="5" value="187"/></column_widths><filter_values/><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table></browse_table_settings></tab_browse><tab_sql><sql name="SQL 1*">public RilevazioneFarmaco[] getRilevazioneFarmaco(String usernamePaziente) throws SQLException {
    ArrayList&lt;RilevazioneFarmaco&gt; rilevazioni = new ArrayList&lt;&gt;();

    String query =
        &quot;SELECT rf.* &quot; +
        &quot;FROM paziente p &quot; +
        &quot;INNER JOIN rilevazione_farmaco rf ON p.id = rf.id_paziente &quot; +
        &quot;WHERE (? IS NULL OR p.username = ?)&quot;;

    super.getConnection().executeQuery(
        query,
        rs -&gt; {
            while (rs.next()) {
                rilevazioni.add(new RilevazioneFarmaco(
                    rs.getInt(&quot;id&quot;),
                    rs.getInt(&quot;id_paziente&quot;),
                    rs.getTimestamp(&quot;timestamp&quot;),
                    rs.getInt(&quot;id_farmaco&quot;),
                    rs.getFloat(&quot;quantita&quot;),
                    rs.getString(&quot;note&quot;)
                ));
            }
            return null;
        },
        usernamePaziente.equals(&quot;ALL&quot;) ? null : usernamePaziente,  // 1st placeholder
        usernamePaziente.equals(&quot;ALL&quot;) ? null : usernamePaziente   // 2nd placeholder
    );

    return rilevazioni.toArray(new RilevazioneFarmaco[0]);
}

